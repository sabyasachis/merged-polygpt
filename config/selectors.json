{
  "chatgpt": {
    "input": [
      "#prompt-textarea",
      "textarea[placeholder*='Message']",
      "textarea[data-testid='chat-input-textarea']",
      "div[contenteditable='true']"
    ],
    "submit": [
      "button[data-testid='send-button']",
      "button[aria-label*='Send']",
      "button[type='submit']"
    ],
    "newChat": [
      "a[href='/']",
      "button[aria-label*='New chat']",
      "a[aria-label*='New chat']",
      "[data-testid='new-chat-button']"
    ],
    "response": [
      "div[data-message-author-role='assistant']",
      "article[data-testid='conversation-turn']:has([data-message-author-role='assistant'])",
      ".agent-turn",
      "div[class*='markdown']",
      "div.response-text",
      "div[data-message-model-slug]"
    ],
    "responseContainer": [
      "main",
      "div[role='presentation']",
      "div[class*='conversation']"
    ],
    "stopButton": [
      "button[data-testid='stop-button']",
      "button[aria-label*='Stop']",
      "button[aria-label*='stop generating']"
    ]
  },
  "gemini": {
    "input": [
      "rich-textarea",
      "div[role='textbox']",
      "[contenteditable='true']",
      "textarea"
    ],
    "submit": [
      "button[aria-label*='Send']",
      "button[data-testid='send-button']",
      "div[class*='send-button'] button"
    ],
    "newChat": [
      "button[aria-label*='New chat']",
      "a[aria-label*='New chat']",
      "button[title*='New chat']",
      "[data-testid='new-chat-button']"
    ],
    "response": [
      "model-response",
      "message-content[data-role='model']",
      "div[class*='model-response']",
      "div[class*='response-container']",
      "div.model-response-text",
      "div[data-test-id*='model-turn']",
      ".response-container-content",
      "div[class*='conversation'] > div[class*='model']"
    ],
    "responseContainer": [
      "main",
      "chat-window",
      "div[class*='conversation']"
    ],
    "stopButton": [
      "button[aria-label*='Stop']",
      "button[aria-label*='stop generating']",
      "button[class*='stop-button']"
    ]
  },
  "perplexity": {
    "input": [
      "#ask-input",
      "div[data-lexical-editor='true']",
      "[contenteditable='true'][role='textbox']",
      "[contenteditable='true']"
    ],
    "submit": [
      "button[data-testid='submit-button']",
      "button[aria-label='Submit']",
      "button[type='submit']"
    ],
    "newChat": [
      "button[aria-label*='New']",
      "a[aria-label*='New']",
      "button[title*='New thread']",
      "[data-testid='new-thread-button']"
    ],
    "response": [
      "div[class*='answer']",
      "div[class*='response']",
      "div[data-testid='perplexity-answer']",
      "div[class*='markdown']"
    ],
    "responseContainer": [
      "main",
      "div[class*='thread']",
      "div[class*='conversation']"
    ],
    "stopButton": [
      "button[aria-label*='Stop']",
      "button[title*='Stop']",
      "button[class*='stop']"
    ]
  },
  "claude": {
    "input": [
      "div[data-testid='chat-input']",
      "div[contenteditable='true'][role='textbox']",
      "div.ProseMirror",
      "[role='textbox']"
    ],
    "submit": [
      "button[aria-label='Send message']",
      "button[aria-label*='Send']",
      "button[type='submit']"
    ],
    "newChat": [
      "button[aria-label*='New']",
      "a[aria-label*='New']",
      "button[title*='New chat']"
    ],
    "response": [
      "div.font-claude-response",
      "div.progressive-markdown",
      "div.standard-markdown",
      "p.font-claude-response-body",
      "div[data-message-author-role='assistant']",
      "div[data-is-streaming='true'][data-message-author-role='assistant']",
      "div[class*='assistant'] div[class*='prose']",
      "[data-testid*='message'][data-message-author-role='assistant']",
      "div[data-is-streaming]",
      "[class*='prose']:not(:has(input))",
      "div.whitespace-pre-wrap"
    ],
    "responseContainer": [
      "main",
      "div[class*='conversation']",
      "div[role='main']",
      "body"
    ],
    "stopButton": [
      "button[aria-label='Stop generating']",
      "button[aria-label*='Stop']",
      "button[class*='stop']"
    ]
  }
}
